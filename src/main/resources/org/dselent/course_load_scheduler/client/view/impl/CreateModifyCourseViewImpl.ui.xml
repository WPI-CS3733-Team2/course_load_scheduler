<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>

	</ui:style>
	<g:HTMLPanel ui:field="createModifyPanel">
		<g:Grid>
			<g:row />
			<g:row>
				<g:customCell>
					<g:Label text="Course Name:" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="courseNameTextBox" />
				</g:customCell>
				<g:customCell>
					<g:Label text="Course Number:" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="courseNumberTextBox" />
				</g:customCell>
				<g:customCell>
					<g:Label text="Frequency:" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="frequencyTextBox" />
				</g:customCell>
			</g:row>
			<g:row />
		</g:Grid>
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:HorizontalPanel width="102px">
						<g:Button ui:field="addSectionBtn" width="101px">Add Section</g:Button>
					</g:HorizontalPanel>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Grid>
						<g:row>
							<g:customCell>
								<g:Label text="Section Name:"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="sectionNameTextBox"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="Year:"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="yearTextBox"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="Days (M, T, W, R, F):"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="daysTextBox"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Label text="CRN:"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="crnTextBox"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="Semester/Term:"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="termTextBox"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Label text="Section Type:"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="typeTextBox"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="Start Time:"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="startTimeTextBox"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Label text="Expected Population: "/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="popTextBox"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="End Time:"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="endTimeTextBox"/>
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
		</g:Grid>
		<g:FlowPanel>
			<g:Grid>
				<g:row>
					<g:customCell>
						<g:FlowPanel height="250px">
							<p1:CellTable ui:field="sectionTable" height="50px"/>
						</g:FlowPanel>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Button ui:field="removeSectionBtn" width="140px">Remove Selected Section</g:Button>
					</g:customCell>
				</g:row>
				<g:row/>
			</g:Grid>
		</g:FlowPanel>
		<g:Grid>
			<g:row />
			<g:row>
				<g:customCell>
					<g:HorizontalPanel width="358px">
						<g:Button width="73px" ui:field="closeBtn">Cancel</g:Button>
					</g:HorizontalPanel>
				</g:customCell>
				<g:customCell>
					<g:HorizontalPanel>
						<g:Button width="77px" ui:field="submitBtn">Submit</g:Button>
					</g:HorizontalPanel>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 