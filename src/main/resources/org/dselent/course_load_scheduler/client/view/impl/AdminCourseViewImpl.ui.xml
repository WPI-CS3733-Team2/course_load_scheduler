<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel ui:field="adminCoursePanel">
		<g:Grid>
			<g:row/>
			<g:row>
				<g:customCell>
					<g:Button text="Add Course" ui:field="addCourseBtn"/>
				</g:customCell>
				<g:customCell>
					<g:Label text="Search Course:"/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="searchCourseTxtBox"/>
				</g:customCell>
				<g:customCell>
					<g:Button text="Search" ui:field="searchCourseButton"/>
				</g:customCell>
			</g:row>
			<g:row/>
		</g:Grid>
		<g:ScrollPanel width="800px" height="500px">
			<g:Grid width="700px" ui:field="allCoursesGrid"/>
		</g:ScrollPanel>
	</g:HTMLPanel>
</ui:UiBinder> 