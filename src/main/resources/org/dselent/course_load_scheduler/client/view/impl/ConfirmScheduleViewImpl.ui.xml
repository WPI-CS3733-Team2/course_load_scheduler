<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel ui:field="confirmSchedulePanel" width="640px">
			<g:Label text="Confirm Schedule" ui:field="confirmScheduleHeader" horizontalAlignment="ALIGN_CENTER" width="640px"/>
			<g:TextArea width="630px" height="73px" ui:field="courseInformationBox" alignment="LEFT" readOnly="true"/>
			<g:HorizontalPanel width="641px" height="54px">
				<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_RIGHT">
					<g:Label text="Schedule Name" ui:field="scheduleNameLabel" horizontalAlignment="ALIGN_CENTER" width="220px"/>
				</g:Cell>
				<g:Cell verticalAlignment="ALIGN_MIDDLE">
					<g:TextBox alignment="LEFT" ui:field="scheduleNameBox" width="196px"/>
				</g:Cell>
			</g:HorizontalPanel>
			<g:Cell horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="confirmScheduleButton" text="Confirm Schedule"/>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 