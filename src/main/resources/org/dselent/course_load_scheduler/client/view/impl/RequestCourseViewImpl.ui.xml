<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel ui:field="requestCoursePanel">
		<g:Grid>
			<g:row/>
			<g:row>
				<g:customCell>
					<g:Label text="Course Name" ui:field="courseNameLabel"/>
				</g:customCell>
				<g:customCell>
					<g:Label text="Course Number" ui:field="courseNumberLabel"/>
				</g:customCell>
			</g:row>
			<g:row/>
		</g:Grid>
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:Label text="Course Sections:"/>
				</g:customCell>
				<g:customCell>
					<g:FlowPanel width="500px" height="200px">
						<p1:CellTable ui:field="sectionTable" height="50px" pageSize="10"/>
					</g:FlowPanel>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Available Times:"/>
				</g:customCell>
				<g:customCell>
					<g:Grid>
						<g:row>
							<g:customCell>
								<g:CheckBox text="8:00 AM - 9:00 AM" ui:field="eightToNineCB"/>
							</g:customCell>
							<g:customCell>
								<g:CheckBox text="9:00 AM - 10:00 AM" ui:field="nineToTenCB"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:CheckBox text="10:00 AM - 11:00 AM" ui:field="tenToElevenCB"/>
							</g:customCell>
							<g:customCell>
								<g:CheckBox text="11:00 AM - 12:00 PM" ui:field="elevenToTwelveCB"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:CheckBox text="12:00 PM - 1:00 PM" ui:field="twelveToOneCB"/>
							</g:customCell>
							<g:customCell>
								<g:CheckBox text="1:00 PM - 2:00 PM" ui:field="oneToTwoCB"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:CheckBox text="2:00 PM - 3: 00 PM" ui:field="twoToThreeCB"/>
							</g:customCell>
							<g:customCell>
								<g:CheckBox text="3:00 PM - 4:00 PM" ui:field="threeToFourCB"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:CheckBox text="4:00 PM - 5:00 PM" ui:field="fourToFiveCB"/>
							</g:customCell>
							<g:customCell>
								<g:CheckBox text="5:00 PM - 6:00 PM" ui:field="fiveToSixCB"/>
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
		</g:Grid>
		<g:Grid>
			<g:row/>
			<g:row>
				<g:customCell>
					<g:FlowPanel width="558px">
						<g:Button width="75px" ui:field="cancelBtn">Cancel</g:Button>
					</g:FlowPanel>
				</g:customCell>
				<g:customCell>
					<g:FlowPanel>
						<g:Button ui:field="submitBtn" width="75px">Submit</g:Button>
					</g:FlowPanel>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 