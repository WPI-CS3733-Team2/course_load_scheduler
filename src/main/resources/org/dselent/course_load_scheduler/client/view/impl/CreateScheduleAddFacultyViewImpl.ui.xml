<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel ui:field="verticalPanel" width="100%">
			<g:Cell width="100%">
				<g:Label text="Add a Faculty Member to Schedule" ui:field="facultyLabel" horizontalAlignment="ALIGN_CENTER"/>
			</g:Cell>
		</g:VerticalPanel>
		<g:HorizontalPanel width="100%">
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<g:Button ui:field="nextPageButton" text="Next"/>
				</g:Cell>
			</g:HorizontalPanel>
	
	</g:HTMLPanel>
</ui:UiBinder> 